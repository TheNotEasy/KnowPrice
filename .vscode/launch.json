{
    "configurations": [
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8100",
            "webRoot": "${workspaceFolder}",
            "userDataDir": false,
            "urlFilter": "http://localhost:8100/*",
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/../YktInfoDjango/manage.py",
            "args": [
                "runserver",
                "0.0.0.0:8000"
            ],
            "django": true,
            "python": "${command:python.interpreterPath}",
        },
        {
            "command": "ionic serve --external",
            "name": "Run ionic server",
            "request": "launch",
            "type": "node-terminal"
        },
    ]
}